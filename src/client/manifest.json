{
    "name": "VC_EXE",
    "version": "1.0",
    "description": "A Google Chrome Extension tool which enables Q&A anytime during a videoconference/webinar.",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://meet.google.com/*"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "VCXtension"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "tabs"
    ]
}
