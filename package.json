  },
  "homepage": "https://github.com/joelleoqiyi/VC_Extension#readme",
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "dotenv": "^8.2.0",
    "mongodb": "^3.5.8",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-cli": "^6.26.0",
    "express": "^4.17.1",
    "socket.io": "^2.3.0"
  }
